packages:
  yum:
    postgresql-devel: 9.2
    rubygems: 1.8
    memcached: 1.4.13

container_commands:
  01_collectstatic:
    command: "./manage.py collectstatic --noinput"
  02_replacewsgi:
    command: "cp .ebextensions/wsgi.conf ../wsgi.conf"

option_settings:
  "aws:elasticbeanstalk:container:python":
    WSGIPath: alsudani/wsgi.py
